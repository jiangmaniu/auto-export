{
  "name": "auto-export",
  "type": "module",
  "version": "0.2.6",
  "packageManager": "pnpm@7.1.5",
  "description": "",
  "author": "sudongyuer",
  "license": "MIT",
  "funding": "https://github.com/sponsors/sudongyuer",
  "homepage": "https://github.com/sudongyuer/auto-export#readme",
  "repository": "https://github.com/sudongyuer/auto-export",
  "bugs": "https://github.com/sudongyuer/auto-export/issues",
  "keywords": [],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./index.mjs": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    },
    "./index.cjs": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": {
    "autoexport": "bin/autoexport.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "unbuild --stub",
    "build": "unbuild",
    "test": "vitest",
    "changeset": "changeset add",
    "version": "changeset version",
    "release": "pnpm run build && pnpm run release:only",
    "release:only": "changeset publish",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --cache",
    "lint-fix": "eslint . --fix",
    "try": "tsx src/index.ts"
  },
  "dependencies": {
    "cac": "^6.7.12",
    "chalk": "^5.0.1",
    "commander": "^9.3.0",
    "unconfig": "^0.3.5"
  },
  "devDependencies": {
    "@changesets/cli": "^2.23.0",
    "@sudongyuer/eslint-config": "^0.1.3",
    "@types/node": "^18.0.0",
    "eslint": "^8.19.0",
    "pascal-case": "^3.1.2",
    "tsx": "^3.6.0",
    "typescript": "^4.7.4",
    "unbuild": "^0.7.4",
    "vite": "^2.9.12",
    "vitest": "^0.15.2"
  }
}
